<Window x:Class="OpenVRNotificationPipe.InputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenVRNotificationPipe"
        mc:Ignorable="d"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner"
        SizeToContent="WidthAndHeight"
        ResizeMode="NoResize"
        Title="Change Value" Height="120.833" Width="163.69">
    <Grid>
        <TextBox Name="textBoxValue" Margin="55,14,10,56">Value</TextBox>
        <Button Name="buttonOk" IsDefault="True" Click="OkButton_Click" Margin="98,54,10,10">OK</Button>
        <Button Name="buttonCancel" IsCancel="True" Margin="10,54,98,10">Cancel</Button>
        <Label x:Name="labelValue" HorizontalContentAlignment="Right" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="45"/>
    </Grid>
</Window>
